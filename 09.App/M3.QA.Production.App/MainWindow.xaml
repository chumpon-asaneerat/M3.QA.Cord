<Window x:Class="M3.QA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:ctrl="clr-namespace:M3.QA.Controls;assembly=M3.QA.Controls"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="M3 Cord QA System"
        Height="450"
        Width="800"
        WindowStartupLocation="CenterScreen"
        WindowState="Maximized"
        Loaded="Window_Loaded"
        Unloaded="Window_Unloaded">
    <Grid>
        <DockPanel LastChildFill="True">
            <ctrl:CordStatusBar x:Name="statusbar" DockPanel.Dock="Bottom" />
            <ContentPresenter x:Name="container" />
        </DockPanel>
    </Grid>
</Window>
